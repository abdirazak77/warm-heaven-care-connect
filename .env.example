# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Authentication Configuration
NEXT_PUBLIC_AUTH_REDIRECT_URL=http://localhost:3000/auth/callback

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development # Change to 'production' in production environment

# Security Configuration
NEXT_PUBLIC_ENABLE_HTTPS=false # Set to true in production
NEXT_PUBLIC_COOKIE_SECURE=false # Set to true in production

# API Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# Logging Configuration
LOG_LEVEL=debug # Set to 'info' or 'error' in production

# Cache Configuration
CACHE_TTL=3600

# Session Configuration
SESSION_SECRET=your-session-secret
SESSION_EXPIRY=7d

# Database Configuration (if needed)
DATABASE_URL=your-database-url

# Email Configuration (if needed)
SMTP_HOST=your-smtp-host
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASSWORD=your-smtp-password
SMTP_FROM_EMAIL=noreply@yourdomain.com

# Storage Configuration (if needed)
STORAGE_BUCKET=your-storage-bucket
STORAGE_REGION=your-storage-region

# Monitoring and Analytics (if needed)
SENTRY_DSN=your-sentry-dsn
ANALYTICS_ID=your-analytics-id